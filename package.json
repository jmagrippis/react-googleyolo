{
  "name": "react-googleyolo",
  "version": "0.1.3",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "startDist": "cross-env NODE_ENV=development webpack --watch",
    "buildDist": "yarn buildDist:umd && yarn buildDist:es",
    "buildDist:umd": "cross-env NODE_ENV=production webpack",
    "buildDist:es":
      "cross-env NODE_ENV=production babel ./src --out-dir ./dist/es",
    "precommit": "lint-staged"
  },
  "main": "dist/index.js",
  "module": "dist/es/react-googleyolo.js",
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-2": "^7.0.0-beta.51",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "react-test-renderer": "^16.2.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --write", "git add"],
    "package.json": ["prettier --write", "git add"]
  },
  "author": "Johnny Magrippis <j@magrippis.com> (https://magrippis.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmagrippis/react-googleyolo"
  },
  "homepage": "https://react-googleyolo.netlify.com",
  "license": "MIT",
  "keywords": [
    "react",
    "googleyolo",
    "hoc",
    "auth",
    "authentication",
    "single sign-on",
    "login"
  ]
}
